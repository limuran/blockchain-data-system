# ðŸ”‘ API Keys Configuration
# Copy this file to .env and fill in your actual keys

# Ethereum Node Providers
REACT_APP_INFURA_API_KEY=your_infura_project_id_here
REACT_APP_ALCHEMY_API_KEY=your_alchemy_api_key_here

# The Graph API
REACT_APP_GRAPH_API_URL=https://api.thegraph.com/subgraphs/name/limuran/usdt-data-tracker

# Default Network Settings
REACT_APP_DEFAULT_NETWORK=sepolia
REACT_APP_DEBUG=false

# Contract Deployment (for scripts only, never expose private keys in frontend)
# PRIVATE_KEY=your_private_key_for_contract_deployment
# ETHERSCAN_API_KEY=your_etherscan_api_key
# SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/your_infura_key

# Development Settings
REACT_APP_ENABLE_MOCK_DATA=true
REACT_APP_MOCK_DELAY=2000

# Production Settings
# REACT_APP_ENABLE_ANALYTICS=true
# REACT_APP_SENTRY_DSN=your_sentry_dsn