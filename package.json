{
  "name": "blockchain-data-system",
  "version": "2.0.0",
  "description": "Modern multi-chain blockchain data storage and query system",
  "main": "src/index.js",
  "scripts": {
    "dev": "webpack serve --config webpack.config.js --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "test": "jest",
    "contract:compile": "npx hardhat compile",
    "contract:test": "npx hardhat test",
    "contract:deploy": "npx hardhat run scripts/deploy.js --network sepolia",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ethers": "^6.8.0",
    "lucide-react": "^0.263.1",
    "@uniswap/v3-sdk": "^3.10.0",
    "@uniswap/sdk-core": "^4.0.7",
    "@uniswap/smart-order-router": "^3.15.0"
  },
  "devDependencies": {
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "postcss": "^8.4.24",
    "postcss-loader": "^7.3.3",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.14",
    "html-webpack-plugin": "^5.5.3",
    "@hardhat/core": "^2.17.0",
    "hardhat": "^2.17.0",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "jest": "^29.5.0",
    "eslint": "^8.43.0",
    "prettier": "^2.8.8"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "web3",
    "react",
    "defi",
    "uniswap",
    "token-transfer"
  ],
  "author": "limuran",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/limuran/blockchain-data-system.git"
  }
}